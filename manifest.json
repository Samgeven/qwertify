{
    "name": "qwertify",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts":  [
        {
            "matches": ["<all_urls>"],
            "js": ["js/main.js"]
        }
    ],
    "icons": { 
        "16": "img/logo16.png",
        "48": "img/logo48.png",
        "128": "img/logo128.png" 
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}